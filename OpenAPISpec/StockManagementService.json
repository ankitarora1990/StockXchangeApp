{
   "openapi":"3.0.1",
   "info":{
      "title":"Stock Management API",
      "version":"v1"
   },
   "paths":{
      "/api/stock-prices":{
         "get":{
            "tags":[
               "StockPrice"
            ],
            "responses":{
               "200":{
                  "description":"Success",
                  "content":{
                     "text/plain":{
                        "schema":{
                           "type":"array",
                           "items":{
                              "$ref":"#/components/schemas/Stock"
                           }
                        }
                     },
                     "application/json":{
                        "schema":{
                           "type":"array",
                           "items":{
                              "$ref":"#/components/schemas/Stock"
                           }
                        }
                     },
                     "text/json":{
                        "schema":{
                           "type":"array",
                           "items":{
                              "$ref":"#/components/schemas/Stock"
                           }
                        }
                     }
                  }
               }
            }
         }
      },
      "/api/stock-prices/{stockCode}":{
         "get":{
            "tags":[
               "StockPrice"
            ],
            "parameters":[
               {
                  "name":"stockCode",
                  "in":"path",
                  "required":true,
                  "schema":{
                     "type":"string"
                  }
               }
            ],
            "responses":{
               "200":{
                  "description":"Success",
                  "content":{
                     "text/plain":{
                        "schema":{
                           "$ref":"#/components/schemas/Stock"
                        }
                     },
                     "application/json":{
                        "schema":{
                           "$ref":"#/components/schemas/Stock"
                        }
                     },
                     "text/json":{
                        "schema":{
                           "$ref":"#/components/schemas/Stock"
                        }
                     }
                  }
               }
            }
         }
      },
      "/api/stock-prices/{stockCode}/history":{
         "get":{
            "tags":[
               "StockPrice"
            ],
            "parameters":[
               {
                  "name":"stockCode",
                  "in":"path",
                  "required":true,
                  "schema":{
                     "type":"string"
                  }
               }
            ],
            "responses":{
               "200":{
                  "description":"Success",
                  "content":{
                     "text/plain":{
                        "schema":{
                           "$ref":"#/components/schemas/StockPriceHistory"
                        }
                     },
                     "application/json":{
                        "schema":{
                           "$ref":"#/components/schemas/StockPriceHistory"
                        }
                     },
                     "text/json":{
                        "schema":{
                           "$ref":"#/components/schemas/StockPriceHistory"
                        }
                     }
                  }
               }
            }
         }
      },
      "/api/stocks":{
         "get":{
            "tags":[
               "Stocks"
            ],
            "responses":{
               "200":{
                  "description":"Success",
                  "content":{
                     "text/plain":{
                        "schema":{
                           "type":"array",
                           "items":{
                              "$ref":"#/components/schemas/Stock"
                           }
                        }
                     },
                     "application/json":{
                        "schema":{
                           "type":"array",
                           "items":{
                              "$ref":"#/components/schemas/Stock"
                           }
                        }
                     },
                     "text/json":{
                        "schema":{
                           "type":"array",
                           "items":{
                              "$ref":"#/components/schemas/Stock"
                           }
                        }
                     }
                  }
               }
            }
         },
         "post":{
            "tags":[
               "Stocks"
            ],
            "requestBody":{
               "content":{
                  "application/json-patch+json":{
                     "schema":{
                        "$ref":"#/components/schemas/StockPriceEntry"
                     }
                  },
                  "application/json":{
                     "schema":{
                        "$ref":"#/components/schemas/StockPriceEntry"
                     }
                  },
                  "text/json":{
                     "schema":{
                        "$ref":"#/components/schemas/StockPriceEntry"
                     }
                  },
                  "application/*+json":{
                     "schema":{
                        "$ref":"#/components/schemas/StockPriceEntry"
                     }
                  }
               }
            },
            "responses":{
               "200":{
                  "description":"Success"
               }
            }
         }
      },
      "/api/stocks/{stockCode}":{
         "get":{
            "tags":[
               "Stocks"
            ],
            "parameters":[
               {
                  "name":"stockCode",
                  "in":"path",
                  "required":true,
                  "schema":{
                     "type":"string"
                  }
               }
            ],
            "responses":{
               "200":{
                  "description":"Success",
                  "content":{
                     "text/plain":{
                        "schema":{
                           "$ref":"#/components/schemas/Stock"
                        }
                     },
                     "application/json":{
                        "schema":{
                           "$ref":"#/components/schemas/Stock"
                        }
                     },
                     "text/json":{
                        "schema":{
                           "$ref":"#/components/schemas/Stock"
                        }
                     }
                  }
               }
            }
         },
         "delete":{
            "tags":[
               "Stocks"
            ],
            "parameters":[
               {
                  "name":"stockCode",
                  "in":"path",
                  "required":true,
                  "schema":{
                     "type":"string"
                  }
               }
            ],
            "responses":{
               "200":{
                  "description":"Success"
               }
            }
         }
      }
   },
   "components":{
      "schemas":{
         "Stock":{
            "type":"object",
            "properties":{
               "StockId":{
                  "type":"integer",
                  "format":"int32"
               },
               "StockCode":{
                  "type":"string",
                  "nullable":true
               },
               "Description":{
                  "type":"string",
                  "nullable":true
               },
               "LatestPrice":{
                  "type":"number",
                  "format":"double"
               }
            },
            "additionalProperties":false
         },
         "StockPriceEntry":{
            "type":"object",
            "properties":{
               "StockCode":{
                  "type":"string",
                  "nullable":true
               },
               "Price":{
                  "type":"number",
                  "format":"double"
               },
               "TimeStamp":{
                  "type":"string",
                  "format":"date-time"
               }
            },
            "additionalProperties":false
         },
         "StockPriceHistory":{
            "type":"object",
            "properties":{
               "StockCode":{
                  "type":"string",
                  "nullable":true
               },
               "PriceHistory":{
                  "type":"object",
                  "additionalProperties":{
                     "type":"number",
                     "format":"double"
                  },
                  "nullable":true
               }
            },
            "additionalProperties":false
         }
      }
   }
}